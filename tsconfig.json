{
    "compilerOptions": {
      "outDir": "./dist/",
      "sourceMap": true,
      "strictNullChecks": true,
      "module": "CommonJS",
      "jsx": "react",
      "target": "ES2019",
      "allowJs": true,
      "esModuleInterop": true,
      "strict": true,
      "skipLibCheck": true,
      "forceConsistentCasingInFileNames": true,
      "moduleResolution": "node"
    },
    "include": [
      "./client/",
      "./server/**/*.ts"
    ]
  }
  